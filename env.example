# 應用程式設定
NODE_ENV=development
APP_PORT=3000

# PostgreSQL 資料庫設定
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=aquacircle
POSTGRES_PORT=5432

# Prisma 資料庫連接 URL
# 格式: postgresql://使用者名稱:密碼@主機:埠/資料庫名稱
# 注意：在 Docker Compose 中使用 postgres 作為主機名
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/aquacircle?schema=public
